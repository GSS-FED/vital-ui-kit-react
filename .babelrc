{
  "presets": ["flow", "react", ["env", {"modules": false}]],
  "plugins": [
    "transform-object-rest-spread",
    "babel-plugin-transform-class-properties",
    ["babel-plugin-root-import", {
      "rootPathSuffix": "./"
    }]
  ],
  "env": {
    "development": {
      "plugins": [["babel-plugin-styled-components", {
        "displayName": true
      }]]
    }
  }
}
