{
  "presets": ["react", ["env", {"modules": false}]],
  "plugins": [
    "babel-plugin-transform-object-rest-spread",
    "babel-plugin-transform-class-properties"
  ],
  "env": {
    "development": {
      "plugins": [["babel-plugin-styled-components", {
        "displayName": true
      }]]
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "react"
      ],
      "plugins": [
        "require-context-hook",
        "babel-plugin-transform-object-rest-spread",
        "babel-plugin-transform-class-properties"
      ]
    }
  }
}
