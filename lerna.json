{
  "lerna": "2.9.1",
  "packages": [
    "packages/*",
    "playground/"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "publish": {
    "access": "public"
  },
  "version": "0.7.0",
  "command": {
    "exec": {
      "ignore": "@gssfed/playground"
    },
    "publish": {
      "ignore": "@gssfed/playground"
    }
  },
  "changelog": {
    "repo": "GSS-FED/vital-ui-kit-react",
    "labels": {
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "internal": ":internal: Internal",
      "breaking change": ":boom: Breaking Change"
    },
    "cacheDir": ".changelog",
    "ignoreCommitters": [
      "renovate"
    ]
  }
}
