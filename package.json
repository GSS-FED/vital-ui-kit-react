{
  "name": "@vital-ui/react-root",
  "description": "Vital UI Kit React",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "reset": "rimraf node_modules/ yarn.lock packages/*/node_modules",
    "clean-all": "yarn cache clean && yarn run reset && yarn run clean:build",
    "bootstrap": "lerna bootstrap && yarn run build",
    "clean:build": "lerna exec -- rimraf dist cjs",
    "build": "lerna run compile --stream",
    "prebuild": "yarn clean:build",
    "watch": "cross-env NODE_ENV=development lerna exec --parallel -- tsc-watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o /build",
    "lint": "tslint -c tslint.json './**/*.{ts,tsx}'",
    "test": "NODE_ENV=test jest --env=jsdom",
    "format": "prettier --write '**/**.{js,jsx,ts,tsx}'",
    "changelog": "lerna-changelog",
    "publish": "lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSS-FED/vital-ui-kit-react.git"
  },
  "contributors": [
    "Eric Yip (https://github.com/ericyip)",
    "Laura Lee (https://github.com/l443018)",
    "ChiFeng (https://github.com/ga021396)",
    "Ryan Chu (https://github.com/jj811208)"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@babel/plugin-transform-runtime": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@babel/register": "7.0.0",
    "@storybook/addon-actions": "5.0.0",
    "@storybook/addon-centered": "5.0.0",
    "@storybook/addon-info": "5.0.0",
    "@storybook/addon-knobs": "5.0.0",
    "@storybook/addon-links": "5.0.0",
    "@storybook/addon-notes": "5.0.0",
    "@storybook/addon-options": "5.0.0",
    "@storybook/addon-storyshots": "5.0.0",
    "@storybook/addon-storysource": "5.0.0",
    "@storybook/addons": "5.0.0",
    "@storybook/react": "5.0.0",
    "@svgr/cli": "3.1.0",
    "@types/classnames": "2.2.7",
    "@types/jest": "24.0.9",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.8.2",
    "@types/react-is": "16.7.1",
    "@types/styled-components": "4.1.12",
    "@types/styled-system": "3.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-styled-components": "1.10.0",
    "circular-dependency-plugin": "5.0.2",
    "copyfiles": "2.1.0",
    "cross-env": "5.2.0",
    "faker": "4.1.0",
    "husky": "1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-specific-snapshot": "2.0.0",
    "jest-styled-components": "6.3.1",
    "lerna": "3.10.5",
    "lerna-changelog": "0.8.2",
    "lint-staged": "8.1.0",
    "markdown-loader-jest": "0.1.1",
    "prettier": "1.16.4",
    "react": "16.8.4",
    "react-docgen-typescript-loader": "3.0.1",
    "react-dom": "16.8.4",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "16.8.4",
    "react-testing-library": "6.0.0",
    "rimraf": "2.6.3",
    "rxjs": "6.3.3",
    "storybook-styled-components": "1.1.3",
    "styled-components": "4.1.3",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "tsc-watch": "1.0.31",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3333"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint -c tslint.json --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
