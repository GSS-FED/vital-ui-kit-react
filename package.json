{
  "name": "@vital-ui/react-root",
  "description": "Vital UI Kit React",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "reset": "rm -rf node_modules/ yarn.lock packages/*/node_modules",
    "clean-all": "yarn cache clean && yarn run reset && yarn run clean:build",
    "bootstrap": "lerna bootstrap && yarn run build",
    "clean:build": "lerna exec -- rm -rf dist cjs",
    "build": "lerna run compile --stream",
    "prebuild": "yarn clean:build",
    "watch": "cross-env NODE_ENV=development lerna exec --parallel -- tsc-watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "tslint -c tslint.json './**/*.{ts,tsx}'",
    "test": "NODE_ENV=test jest --env=jsdom",
    "format": "prettier --write '**/**.{js,jsx,ts,tsx}'",
    "changelog": "lerna-changelog",
    "publish": "lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSS-FED/vital-ui-kit-react.git"
  },
  "contributors": [
    "Eric Yip (https://github.com/ericyip)",
    "Laura Lee (https://github.com/l443018)",
    "ChiFeng (https://github.com/ga021396)",
    "Ryan Chu (https://github.com/jj811208)",
    "CJies Tan (https://github.com/cjies)"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-external-helpers": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@babel/register": "7.8.6",
    "@storybook/addon-actions": "5.3.17",
    "@storybook/addon-centered": "5.3.17",
    "@storybook/addon-info": "5.3.17",
    "@storybook/addon-knobs": "5.3.17",
    "@storybook/addon-links": "5.3.17",
    "@storybook/addon-notes": "5.3.17",
    "@storybook/addon-options": "5.3.17",
    "@storybook/addon-storyshots": "5.3.17",
    "@storybook/addon-storysource": "5.3.17",
    "@storybook/addons": "5.3.17",
    "@storybook/react": "5.3.17",
    "@storybook/theming": "5.3.17",
    "@svgr/cli": "5.2.0",
    "@testing-library/react": "10.0.1",
    "@types/classnames": "2.2.10",
    "@types/jest": "25.1.4",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-is": "16.7.1",
    "@types/styled-components": "4.4.3",
    "@types/styled-system": "5.1.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.7",
    "circular-dependency-plugin": "5.2.0",
    "copyfiles": "2.2.0",
    "cross-env": "7.0.2",
    "faker": "4.1.0",
    "husky": "4.2.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "jest-specific-snapshot": "3.0.0",
    "jest-styled-components": "6.3.4",
    "lerna": "3.20.2",
    "lerna-changelog": "1.0.1",
    "lint-staged": "10.0.8",
    "markdown-loader-jest": "0.1.1",
    "prettier": "1.19.1",
    "react": "16.13.1",
    "react-docgen-typescript-loader": "3.7.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "rxjs": "6.5.4",
    "storybook-styled-components": "1.1.4",
    "styled-components": "4.4.1",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "tsc-watch": "4.2.3",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "6.1.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.1.0",
    "typescript": "3.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint -c tslint.json --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
