{
  "name": "@vital-ui/react-root",
  "description": "Vital UI Kit React",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "reset": "rm -rf node_modules/ yarn.lock packages/*/node_modules",
    "clean-all": "yarn cache clean && yarn run reset && yarn run clean:build",
    "bootstrap": "lerna bootstrap",
    "clean:build": "lerna exec -- rimraf dist",
    "build": "lerna run --stream -- compile",
    "prebuild": "yarn clean:build",
    "watch": "cross-env NODE_ENV=development lerna exec --parallel -- tsc-watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o /public",
    "test": "jest",
    "flow-typed": "rm -rf flow-typed && flow-typed install -s",
    "format": "prettier --write '**/**.{js,jsx,ts,tsx}'",
    "lint": "eslint packages/**; exit 0",
    "lint:watch": "esw -w packages/**",
    "changelog": "lerna-changelog",
    "publish": "lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSS-FED/vital-ui-kit-react.git"
  },
  "contributors": [
    "Eric Yip (https://github.com/ericyip)",
    "Laura Lee (https://github.com/l443018)"
  ],
  "license": "MIT",
  "devDependencies": {
    "@gssfed/eslint-config": "0.0.4",
    "@storybook/addon-actions": "4.0.0-alpha.16",
    "@storybook/addon-centered": "4.0.0-alpha.16",
    "@storybook/addon-info": "4.0.0-alpha.16",
    "@storybook/addon-knobs": "4.0.0-alpha.16",
    "@storybook/addon-links": "4.0.0-alpha.16",
    "@storybook/addon-notes": "4.0.0-alpha.16",
    "@storybook/addon-options": "4.0.0-alpha.16",
    "@storybook/addon-storyshots": "4.0.0-alpha.16",
    "@storybook/addons": "4.0.0-alpha.16",
    "@storybook/react": "4.0.0-alpha.16",
    "@svgr/cli": "^2.2.0",
    "@types/classnames": "^2.2.6",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/react-is": "^16.3.1",
    "@types/styled-system": "^3.0.2",
    "awesome-typescript-loader": "^5.2.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-inline-import-data-uri": "1.0.1",
    "babel-plugin-react-docgen": "1.9.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "circular-dependency-plugin": "^5.0.2",
    "copyfiles": "^2.0.0",
    "cross-env": "5.2.0",
    "eslint": "5.4.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "faker": "4.1.0",
    "flow-copy-source": "2.0.2",
    "husky": "1.0.0-rc.13",
    "jest": "23.4.2",
    "jest-specific-snapshot": "1.0.0",
    "jest-styled-components": "5.0.1",
    "lerna": "2.11.0",
    "lerna-changelog": "0.8.0",
    "lint-staged": "7.2.0",
    "markdown-loader-jest": "0.1.1",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.0",
    "react": "16.4.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "16.4.1",
    "react-test-renderer": "16.4.1",
    "rimraf": "2.6.2",
    "rollup": "0.58.2",
    "rollup-plugin-auto-external": "1.2.0",
    "rollup-plugin-babel": "3.0.4",
    "rollup-plugin-commonjs": "9.1.3",
