    "@vital-ui/react-utils": "^0.10.6",
    "classnames": "^2.2.6",
    "rc-trigger": "v4.0.2"
  },
  "devDependencies": {
    "less": "3.11.3",
    "typescript": "3.8.3"
  }
}
