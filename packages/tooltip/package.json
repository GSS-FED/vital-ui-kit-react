{
  "name": "@vital-ui/react-tooltip",
  "version": "0.10.6",
  "description": "",
  "main": "cjs/index.js",
  "module": "dist/index.js",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "clean": "rm -rf dist cjs",
    "compile:less": "lessc --include-path=../../node_modules ./src/index.less ./src/index.css",
    "compile:copy-dist": "cp ./src/index.css ./dist/index.css",
    "compile:copy-cjs": "cp ./src/index.css ./cjs/index.css",
    "compile:copy": "npm run compile:copy-dist && npm run compile:copy-cjs",
    "compile": "tsc && tsc --module commonjs --outdir cjs && npm run compile:less && npm run compile:copy"
  },
  "author": "",
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16",
    "styled-components": "*"
  },
  "dependencies": {
    "@vital-ui/react-theme": "^0.10.6",
    "classnames": "^2.2.6",
    "rc-trigger": "v4.3.0"
  },
  "devDependencies": {
    "copyfiles": "2.3.0",
    "less": "3.11.3",
    "typescript": "3.8.3"
  }
}
