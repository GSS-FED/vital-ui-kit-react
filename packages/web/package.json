{
  "name": "@vital-ui/react",
  "version": "0.8.7",
  "description": "Vital UI Kit for React!",
  "main": "cjs/index.js",
  "module": "dist/index.js",
  "style": "./dist/global.css",
  "scripts": {
    "clean": "rm -rf dist lib",
    "copy": "copyfiles src/*.css dist -u 1 && copyfiles src/*.css lib -u 1",
    "compile": "tsc && tsc --module commonjs --outdir cjs && yarn run copy"
  },
  "sideEffects": [
    "*.css"
  ],
  "contributors": [
    "Eric Yip (https://github.com/ericyip)",
    "Laura Lee (https://github.com/l443018)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSS-FED/vital-ui-kit-react.git"
  },
  "keywords": [
    "vital",
    "react",
    "ui",
    "component",
    "web"
  ],
  "dependencies": {
    "@vital-ui/react-avatar": "^0.8.6",
    "@vital-ui/react-badge": "^0.8.6",
    "@vital-ui/react-button": "^0.8.6",
    "@vital-ui/react-card": "^0.8.6",
    "@vital-ui/react-form": "^0.8.7",
    "@vital-ui/react-icon": "^0.8.6",
    "@vital-ui/react-layout": "^0.8.6",
    "@vital-ui/react-list": "^0.8.6",
    "@vital-ui/react-menu": "^0.8.6",
    "@vital-ui/react-modal": "^0.8.6",
    "@vital-ui/react-pagination": "^0.8.6",
    "@vital-ui/react-pill": "^0.8.6",
    "@vital-ui/react-tabs": "^0.8.6",
    "@vital-ui/react-theme": "^0.8.6",
    "@vital-ui/react-tooltip": "^0.8.6",
    "@vital-ui/react-utils": "^0.8.6"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16",
    "styled-components": ">=2 || >=3"
  },
  "devDependencies": {
    "copyfiles": "^2.0.0",
    "typescript": "^3.0.1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GSS-FED/vital-ui-kit-react/issues"
