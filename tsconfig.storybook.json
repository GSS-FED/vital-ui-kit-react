{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": false,
    "declaration": false,
    "noUnusedParameters": false,
    "rootDir": ".",
    "baseUrl": "./"
  },
  "include": [
    "packages/**/*"
  ],
  "exclude": ["node_modules"]
}
