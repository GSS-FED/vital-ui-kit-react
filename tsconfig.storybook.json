{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": false,
    "declaration": false,
    "rootDir": ".",
    "outDir": "dist",
    "baseUrl": "."
  },
  "include": [
    "packages", "stories"
  ],
  "exclude": ["node_modules"]
}
